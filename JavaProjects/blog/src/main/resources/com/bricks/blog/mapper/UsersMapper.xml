<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bricks.blog.mapper.UsersMapper">

    <resultMap id="BaseResultMap" type="com.bricks.blog.entities.Users">
            <id property="id" column="id" />
            <result property="username" column="username" />
            <result property="password" column="password" />
            <result property="email" column="email" />
            <result property="createTime" column="create_time" />
            <result property="updateTime" column="update_time" />
            <result property="firstName" column="first_name" />
            <result property="lastName" column="last_name" />
            <result property="birth" column="birth" />
            <result property="selfEvaluation" column="self_evaluation" />
    </resultMap>

    <sql id="Base_Column_List">
        id,username,password,email,create_time,update_time,
        first_name,last_name,birth,self_evaluation
    </sql>
</mapper>
